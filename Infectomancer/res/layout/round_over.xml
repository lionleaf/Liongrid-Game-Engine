<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/roundOverMainLayout"
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Return to main menu" android:layout_alignParentBottom="true" android:id="@+id/returnButton"></Button>
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="Round over!" android:id="@+id/roundOverText" android:textAppearance="?android:attr/textAppearanceLarge" android:layout_alignParentTop="true"></TextView>
    
</RelativeLayout>
